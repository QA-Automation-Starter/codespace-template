{
    "name": "Java 11",
    "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
    "features": {
        "ghcr.io/devcontainers/features/java:1": {
          "version": "11",
          "installMaven": "true",
          "installGradle": "false"
        },
    "secrets": {
      "NAME_OF_SECRET_1": {
        "description": "This is the description of the secret.",
        "documentationUrl": "https://example.com/link/to/info"
      }
    },
    "postCreateCommand": [
        "md foo",
        "java -version"
    ],
    "customizations": {
        "codespaces": {
            "openFiles": [
                "README.md"
            ]
        },
        "vscode": {
            "settings": {},
            "extensions": []
        }
    }
}
